<!DOCTYPE html>
<html>
<head>
  <title>MuttWhat</title>
  <%= stylesheet_link_tag ('../foundation/stylesheets/foundation.css') %>
  <%= stylesheet_link_tag ('../slideshow/slick.min.css') %>
  <%= stylesheet_link_tag ('../slideshow/slick-theme.min.css') %>
  <%= stylesheet_link_tag ('https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick.min.css') %>
  <%= stylesheet_link_tag ('https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick-theme.min.css') %>
  <%= stylesheet_link_tag    fingerprinted_asset('application'), media: 'all' %>
  <%= javascript_include_tag fingerprinted_asset('application') %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render "layouts/nav" %>

<%= yield %>

  <%= javascript_include_tag "../foundation/javascript/vendor/jquery.js" %>
  <%= javascript_include_tag "../foundation/javascript/vendor/what-input.js" %>
  <%= javascript_include_tag "../foundation/javascript/vendor/foundation.js" %>
  <%= javascript_include_tag "../foundation/javascript/app.js" %>
</body>
</html>
